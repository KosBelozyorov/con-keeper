    "url": "https://github.com/KosBelozyorov/con-keeper/issues"
  },
  "homepage": "https://github.com/KosBelozyorov/con-keeper#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.14"
  },
  "devDependencies": {
    "@mate-academy/eslint-config": "0.0.12",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
